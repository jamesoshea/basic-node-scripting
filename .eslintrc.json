{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "no-console": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "prettier/prettier": ["error"]
  },
  "globals": {
    "__ENV": "readonly"
  }
}
